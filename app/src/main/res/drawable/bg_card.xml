<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <solid android:color="#B3888C8C" />
            <stroke
                android:width="1dp"
                android:color="#D5DBDC" />
            <corners android:radius="10dp" />

        </shape>
    </item>

    <item
        android:bottom="2dp"
        android:left="2dp"
        android:top="-4dp"
        android:right="2dp">
        <shape>
            <solid android:color="#FFFFFF" />

            <corners android:radius="10dp" />

        </shape>
    </item>
</layer-list>